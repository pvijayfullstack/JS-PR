

function msort(arr){
    
    for(var i =0; i <arr.length;i++){
        
        for(var j= i+1; j < arr.length; j++){
            
           if(arr[i] > arr[j]) {
               
               swap = arr[j]
               
               arr[j] = arr[i]
               
               arr[i] = swap
               
               
           }
        }
    }
return arr;
}

r = [3, 4, 5, 6, 1]
result = msort(r)
console.log(result)
